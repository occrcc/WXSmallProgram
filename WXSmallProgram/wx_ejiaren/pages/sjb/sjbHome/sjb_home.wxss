.baseContainer {
  display: flex;
  background: rgba(0, 68, 126, 1);
  width: 100%;
}

.warp {
  width: 100%;
  margin: 0;
  padding: 0;
  display: flex;
  flex-direction: column;
  align-items: center;
}


.warp image {
  width: 100%;
  height: 2100rpx;
}

.base_scroll {
  position: absolute;
  width: 100%;
  background: transparent;
  top: 0;
  display: flex;
  align-items: center;
  flex-direction: column;
}

.headView {
  width: 90%;
  height: 270rpx;
  background: linear-gradient(to top, #042263, #0d6cb5);
  top: 100rpx;
  box-shadow: 10rpx 0rpx 15rpx rgba(0, 0, 0, 0.1);
  border-radius: 22rpx;
  border: solid #bdb289 5rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  position: relative;
}

.headLogoView {
  width: 100%;
  height: 120rpx;
  background: transparent;
  flex-direction: row;
  display: flex;
  justify-content: center;
  align-items: center;
}

.headImgViewStyle {
  width: 100%;
  height: 255rpx;
  background: transparent;
  flex-direction: row;
  display: flex;
  justify-content: center;
  align-items: center;
  top:120rpx;
}
.headImgViewStyle image{
  width: 100%;
  height: 375rpx;
}


.ognzLogoStyle {
  width: 74rpx;
  background: transparent;
  flex-direction: column;
  display: flex;
  justify-content: center;
  align-items: center;
}

.ognzLogoStyle image {
  width: 46rpx;
  height: 46rpx;
  /* border-radius: 6rpx;
  border: solid 3rpx rgb(197, 174, 135); */
}

.ognzLogoStyle text {
  font-size:14rpx;
  color: #FFFFFE;
  text-align: center;
  margin-top: 12rpx;
}

.headView image {
  width: 360rpx;
  height: 90rpx;
  margin-top: -42rpx;
}

.headView view {
  width: 90%;
  height: 90%;
  font-size: 26rpx;
  color: white;
  line-height: 50rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  text-align: center;
  flex-direction: column;
}

.titleViewTextStyle {
  color: #799fed;
  font-size: 26rpx;
  font-weight: bold;
}

.titleView {
  width: 90%;
  height: 300rpx;
  margin-top: 10rpx;
}

.titleView view {
  display: flex;
  flex-direction: row;
  width: 100%;
}

.titleView view image {
  width: 132rpx;
  height: 30rpx;
}

.titleView view text {
  width: 132rpx;
  height: 30rpx;
}

.contentViewStyle {
  width: 90%;
  /* height: 700rpx; */
  background: linear-gradient(to top, #042263, #0d6cb5);
  top: 180rpx;
  box-shadow: 10rpx 0rpx 15rpx rgba(0, 0, 0, 0.1);
  border-radius: 22rpx;
  border: solid #bdb289 5rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  position: relative;
}

.contentViewStyle image {
  width: 460rpx;
  height: 90rpx;
  margin-top: -42rpx;
}

.contentView {
  width: 94%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-between;
  align-items: center;
  margin-top: 40rpx;
  padding-top: 20rpx;
}

.contentItem {
  width: 32%;
  /* height: 140rpx; */
  margin-bottom: 55rpx;
  display: flex;
  position: relative;
  justify-content: center;
}

.contentItem button {
  width: 190rpx;
  height: 180rpx; 
  margin: 0;
  color: #e1d3a0;
  font-size: 30rpx;
  text-align: center;
  font-weight: bold;
  display: flex;
  flex-direction: column;
  align-items: center;
  background: transparent;
}

.contentItem button image {
  width: 190rpx;
  height: 94rpx;
  margin: 0;
  position: absolute;
  top: 0;
}

 .contentItem button text {
  width: 190rpx;
  margin: 0;
  position: absolute;
  top:86rpx;
} 

 .contentItem button view {
  width: 190rpx;
  height: 140rpx;
  position: absolute;
  top: 0;
  display: flex;
  
} 

.contentItem button view image {
  width: 190rpx;
  height: 94rpx;
  position: absolute;
  top: 0;
}

.sharBtnStyle {
  width: 426rpx;
  height: 95rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background: transparent;
}

.sharBtnStyle image {
  width: 426rpx;
  height: 95rpx;
  position: absolute;
}

.sharBtnStyle view {
  width: 426rpx;
  height: 95rpx;
  font-size:30rpx;
  font-weight: bold;
  color: white;
  position: absolute;
  display: flex;
  justify-content: center;
  padding-top: 10rpx;

}

.sharView {
  width: 90%;
  height: 100rpx;
