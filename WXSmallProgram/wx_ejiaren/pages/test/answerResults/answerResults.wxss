.baseContainer {
  width: 100%;
  height: 100%;
  background: #fedb57;
  position: fixed;
  display: flex;
  align-items: center;
  flex-direction: column;
}

.base_scroll {
  position: fixed;
  width: 100%;
  height: 100%;
  background: transparent;
  top: 0;
  /* z-index: 999; */
}

.headView {
  width: 100%;
  flex-direction: row;
  display: flex;
  margin-top: 30rpx;
}

.ognzLogo {
  width: 100rpx;
  height: 100rpx;
  margin-left: 30rpx;
  border-radius: 50rpx;
}

.biaoStyle {
  flex-direction: row;
  display: flex;
  width: 76%;
  margin-top: 14rpx;
  
}

.logoStyle {
  width: 20%;
  height: 20%;
}

.textImgStyle {
  width: 580rpx;
  height: 265rpx;
}

.cententImg {
  width: 100%;
  height: 100rpx;
  display: flex;
  justify-content: center;
  margin-top: 60rpx;
}
.cententImg image{
  width: 564rpx;
  height: 120rpx;
}

.codeImgStyle {
  width: 100%;
  height: 250rpx;
  display: flex;
  justify-content: center;
  margin-top: 50rpx;
  align-items: center;
}

.codeImgStyle image{
  width: 250rpx;
  height: 250rpx;
}

.footImgStyle {
  width: 110%;
  height: 540rpx;
  position: absolute;
  bottom: -200rpx;
  z-index: 0;
}

.cavasStyle {
  margin-top: -660rpx;
  width:140px;
  height:140px;
}

.footBtnView {
  width: 100%;
  height: 140rpx;
  position: absolute;
  bottom: 0;
  display: flex;
  justify-content: space-around;
  align-items: center;
  background: transparent;
  z-index: 666;
}

.footBtnView view {
  width: 40%;
  height: 80rpx;
  box-shadow: 8rpx 8rpx 15rpx rgba(66, 58, 27, 1);
  background: #E86363;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 10rpx;
}

.sharBtnStyle {
  font-size:30rpx;
  color: white;
  font-weight: bold;
  display: flex;
  justify-content: center;
  align-items: center;
  background: transparent;
  flex-direction: column;
  line-height: 34rpx;
  
}

.tipView {
  width: 100%;
  height: 103%;
  top: -30rpx;
  position: fixed;
  background: rgba(0, 0, 0, 0.5);
  z-index: 9999;
  font-size: 18rpx;
  display: flex;
}

.showQView {
  width: 80%;
  height: 80%;
  margin-left: 10%;
  margin-top: 10%;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.tipImage{
    width: 750rpx;
    height: 1334rpx;
}

.saveImgView {
    margin-top: 24rpx;
    border-radius: 6rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
    font-size: 28rpx;
    font-weight: bold;
    color: white;
    flex-direction: row;
}

.saveBtnView {
    display: flex;
    align-items: center;
    text-align: center;
    justify-content: center;
    width: 200rpx;
    background: #A390F2;
    height: 80rpx;
    border-radius: 6rpx;
    font-size: 28rpx;
    font-weight: normal;
    color: white;
    margin-top: 10rpx;
}

button::after{
  border: none;
}
